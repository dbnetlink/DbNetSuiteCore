@page
@using DbNetSuiteCore.Components;
@{
    DbNetComboCore employeesCombo = new DbNetComboCore("northwind", "employees", "employeeid", "lastname || ', ' || firstname");
    employeesCombo.DataOnlyColumns = new List<string>() { "ReportsTo", "Title", "TitleOfCourtesy", "BirthDate", "HireDate", "Address", "City", "Region", "PostalCode", "Country", "HomePhone", "Extension" };
    @employeesCombo.Render()
}