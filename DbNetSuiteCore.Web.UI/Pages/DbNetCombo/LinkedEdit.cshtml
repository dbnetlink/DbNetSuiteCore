@page
@using DbNetSuiteCore.Components;
@using DbNetSuiteCore.Enums;
@using DbNetSuiteCore.Enums.DbNetGrid;
@{
    DbNetEditCore employeesEdit = new DbNetEditCore("northwind", "employees");
    DbNetComboCore employeesCombo = new DbNetComboCore("northwind", "employees", "employeeid", "lastname || ', ' || firstname") ;
    employeesCombo.AddLinkedControl(employeesEdit);
    @employeesCombo.Render()
}