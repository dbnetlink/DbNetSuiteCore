@page
@using DbNetSuiteCore.Components;
@{
    // Configure the grid and render
    DbNetGridCore customersGrid = new DbNetGridCore("northwind","customers");
    customersGrid.SetColumnLookup("country");
    customersGrid.SetColumnLookup("region");
    customersGrid.QuickSearch = true;
    customersGrid.Update = true;
    @customersGrid.Render()
}