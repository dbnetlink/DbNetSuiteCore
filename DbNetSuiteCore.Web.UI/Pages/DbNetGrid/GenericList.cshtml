@page
@using DbNetSuiteCore.Components;
@using DbNetSuiteCore.Web.UI.Models;
@{
    List<Product> products = new List<Product>()
    {
        new Product() {Id = 1, Name = "Seville Oranges", Description = "Imported from Spain", Created = new DateTime(2023,2,1), Discontinued = false, MinimumReorderLevel = 10, UnitPrice = 0.55M},
        new Product() {Id = 1, Name = "Coxs Apples", Description = "Home grown in Kent", Created = new DateTime(2021,8,22), Discontinued = true, MinimumReorderLevel = 8, UnitPrice = 0.33M},
        new Product() {Id = 1, Name = "Cauliflower", Description = "Home grown in Spalding", Created = new DateTime(2019,8,22), Discontinued = false, MinimumReorderLevel = 5, UnitPrice = 1.55M}
    };
    DbNetGridCore ordersGrid = new DbNetGridCore();
    ordersGrid.AddList(products, HttpContext);
    @ordersGrid.Render()

    DbNetGridCore productsGrid = new DbNetGridCore();
    productsGrid.AddList(products, HttpContext);
    @productsGrid.Render()
}