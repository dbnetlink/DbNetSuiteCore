@page
@model GenericListModel
@using DbNetSuiteCore.Components;
@using DbNetSuiteCore.Enums;
@using DbNetSuiteCore.Web.UI.Models;
@using DbNetSuiteCore.Web.UI.Pages.Samples.DbNetEdit;
@{
    DbNetEditCore employeesEdit = new DbNetEditCore(DataSourceType.List);
    employeesEdit.AddList(Model.Employees, HttpContext);
    employeesEdit.Column(nameof(Employee.Gender)).Lookup<string>(new Dictionary<string, string>() { { "M", "Male" }, { "F", "Female" } });
    employeesEdit.Column(nameof(Employee.PayFrequency)).Lookup<int>(new Dictionary<int, string>() { { 1, "Weekly" }, { 2, "Monthly" } });
    employeesEdit.Column(nameof(Employee.EmployeeKey)).PrimaryKey();
 //   employeesEdit.Column(new string[] { nameof(Employee.LastName), nameof(Employee.FirstName), nameof(Employee.EmailAddress), nameof(Employee.Gender) }).Browse();
    employeesEdit.LayoutColumns = 2;

    @employeesEdit.Render()
}